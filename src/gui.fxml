<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="Support">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Join ROBLOX Group" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <AnchorPane fx:id="stop" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Label alignment="CENTER" layoutX="155.0" layoutY="177.0" style="&#10;" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <Button fx:id="startButton" defaultButton="true" layoutX="97.0" layoutY="254.0" mnemonicParsing="false" onAction="#startClicked" prefHeight="56.0" prefWidth="117.0" text="Start" />
                <Button fx:id="stopButton" disable="true" layoutX="268.0" layoutY="254.0" mnemonicParsing="false" onAction="#stopClicked" prefHeight="56.0" prefWidth="117.0" text="Stop" />
                <Button fx:id="cancelButton" disable="true" layoutX="439.0" layoutY="254.0" mnemonicParsing="false" onAction="#cancelClicked" prefHeight="56.0" prefWidth="117.0" text="Cancel" />
                <TextField fx:id="startField" accessibleText="Enter the starting value, &gt;= 1." layoutX="166.0" layoutY="25.0" prefHeight="24.0" prefWidth="96.0" promptText="Start" />
                <TextField fx:id="endField" layoutX="385.0" layoutY="25.0" prefHeight="24.0" prefWidth="96.0" promptText="End" />
                <Text layoutX="263.0" layoutY="217.0" stroke="#b21d1d" strokeType="OUTSIDE" strokeWidth="0.0" text="Twenty Five">
                    <font>
                        <Font size="21.0" />
                    </font>
                </Text>
                <Text layoutX="301.0" layoutY="192.0" opacity="0.3" strokeType="OUTSIDE" strokeWidth="0.0" text="Preview" />
                <RadioButton layoutX="111.0" layoutY="68.0" mnemonicParsing="false" selected="true" text="Camel Case">
                    <toggleGroup>
                        <ToggleGroup fx:id="textcase" />
                    </toggleGroup>
                </RadioButton>
                <RadioButton layoutX="220.0" layoutY="68.0" mnemonicParsing="false" text="UPPER CASE" toggleGroup="$textcase" />
                <RadioButton layoutX="335.0" layoutY="68.0" mnemonicParsing="false" text="Normal case" toggleGroup="$textcase" />
                <RadioButton layoutX="447.0" layoutY="68.0" mnemonicParsing="false" text="lower case" toggleGroup="$textcase" />
                <RadioButton layoutX="112.0" layoutY="111.0" mnemonicParsing="false" text="Full Stop" />
                <Text layoutX="20.0" layoutY="82.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Case" />
                <Text layoutX="20.0" layoutY="125.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Punctuation" />
                <RadioButton layoutX="222.0" layoutY="111.0" mnemonicParsing="false" text="Exclamation" />
            </children>
        </AnchorPane>
    </children>
</VBox>
